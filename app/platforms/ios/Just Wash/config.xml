<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gsprasad.justwashapi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="PayPalMobile">
        <param name="ios-package" onload="true" value="PayPalMobileCordovaPlugin" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <name>Just Wash</name>
    <description>
        Just Wash is a car washing company which offers door step services by ordering through a mobile application.
    </description>
    <author email="support@justwash.com" href="http://bharat99.com">
        Just Wash Team
    </author>
    <content src="index.html" />
    <preference name="splashscreen" value="none" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="white-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="false" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <gap:plugin name="org.apache.cordova.battery-status" />
    <gap:plugin name="org.apache.cordova.camera" />
    <gap:plugin name="org.apache.cordova.media-capture" />
    <gap:plugin name="org.apache.cordova.console" />
    <gap:plugin name="org.apache.cordova.contacts" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.device-motion" />
    <gap:plugin name="org.apache.cordova.device-orientation" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <gap:plugin name="org.apache.cordova.file" />
    <gap:plugin name="org.apache.cordova.file-transfer" />
    <gap:plugin name="org.apache.cordova.globalization" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <gap:plugin name="org.apache.cordova.media" />
    <gap:plugin name="org.apache.cordova.network-information" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <gap:plugin name="org.apache.cordova.vibration" />
    <gap:plugin name="org.apache.cordova.geolocation" />
    <gap:plugin name="com.paypal.cordova.mobilesdk" />
    <icon src="icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/icon.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/icon.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/icon.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/icon.png" />
    <icon gap:platform="blackberry" src="www/icon.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/icon.png" />
    <icon gap:platform="ios" height="57" src="www/icon.png" width="57" />
    <icon gap:platform="ios" height="72" src="www/icon.png" width="72" />
    <icon gap:platform="ios" height="114" src="www/icon.png" width="114" />
    <icon gap:platform="ios" height="144" src="www/icon.png" width="144" />
    <icon gap:platform="webos" src="www/icon.png" />
    <icon gap:platform="winphone" src="www/icon.png" />
    <icon gap:platform="winphone" gap:role="background" src="www/icon.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="www/icon.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="www/icon.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="www/icon.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="www/icon.png" />
    <gap:splash gap:platform="blackberry" src="www/icon.png" />
    <gap:splash gap:platform="ios" height="480" src="www/icon.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/icon.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="www/icon.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="www/icon.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="www/icon.png" width="1024" />
    <gap:splash gap:platform="winphone" src="www/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
