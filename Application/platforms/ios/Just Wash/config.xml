<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gsprasad.justwashapi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="PayPalMobile">
        <param name="ios-package" onload="true" value="PayPalMobileCordovaPlugin" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <name>Just Wash</name>
    <description>
        Just Wash is a car washing company which offers door step services by ordering through a mobile application.
    </description>
    <author email="support@justwash.com" href="http://bharat99.com">
        Just Wash Team
    </author>
    <content src="index.html" />
    <preference name="splashscreen" value="splash" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="white-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="false" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="splashScreenDelay" value="5000" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.device-orientation" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <gap:plugin name="org.apache.cordova.core.geolocation" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <gap:plugin name="org.apache.cordova.network-information" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <gap:plugin name="org.apache.cordova.vibration" />
    <gap:plugin name="com.paypal.cordova.mobilesdk" />
    <icon src="icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/icon.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/icon.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/icon.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/icon.png" />
    <icon gap:platform="blackberry" src="www/icon.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/icon.png" />
    <icon gap:platform="ios" height="57" src="www/icon.png" width="57" />
    <icon gap:platform="ios" height="72" src="www/icon.png" width="72" />
    <icon gap:platform="ios" height="114" src="www/icon.png" width="114" />
    <icon gap:platform="ios" height="144" src="www/icon.png" width="144" />
    <icon gap:platform="webos" src="www/icon.png" />
    <icon gap:platform="winphone" src="www/icon.png" />
    <icon gap:platform="winphone" gap:role="background" src="www/icon.png" />
    <splash height="480" platform="ios" src="Default.png" width="320" />
    <splash height="960" platform="ios" src="Default@2x.png" width="640" />
    <splash height="1136" platform="ios" src="Default-568h@2x.png" width="640" />
    <splash height="1334" platform="ios" src="Default-667h@2x.png" width="750" />
    <splash height="2208" platform="ios" src="Default-Portrait-736h@3x.png" width="1242" />
    <splash height="1242" platform="ios" src="Default-Landscape-736h@3x.png" width="2208" />
    <splash height="1024" platform="ios" src="Default-Portrait.png" width="768" />
    <splash height="768" platform="ios" src="Default-Landscape.png" width="1024" />
    <splash height="2048" platform="ios" src="Default-Portrait@2x.png" width="1536" />
    <splash height="1536" platform="ios" src="Default-Landscape@2x.png" width="2048" />
    <splash platform="android" qualifier="ldpi" src="ldpi.png" />
    <splash platform="android" qualifier="mdpi" src="mdpi.png" />
    <splash platform="android" qualifier="hdpi" src="hdpi.png" />
    <splash platform="android" qualifier="xhdpi" src="xhdpi.png" />
    <splash platform="android" qualifier="fr-xhdpi" src="fr-xhdpi.png" />
    <splash platform="android" qualifier="port-xxhdpi" src="portrait-xxhdpi.png" />
    <splash platform="android" qualifier="land-xxhdpi" src="landscape-xxhdpi.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
